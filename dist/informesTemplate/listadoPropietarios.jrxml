	<queryString><![CDATA[SELECT DISTINCT(prop_codigo),prop_nombre,prop_domicilio,prop_telefono,prop_localidad,(case (prop_cuit) WHEN 0 THEN "" ELSE prop_cuit END) as prop_cuit

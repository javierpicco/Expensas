(CASE conc_descripcion WHEN 0 THEN "No" ELSE "Si" END) as descripcion,
(CASE conc_ig WHEN 0 THEN "No" ELSE "Si" END) as ig,
(CASE conc_iva WHEN 0 THEN "No" ELSE "Si" END) as iva,
ifnull(DATE_FORMAT(conc_fecha_fin,'%d-%m-%y'),"") as conc_fecha_fin
